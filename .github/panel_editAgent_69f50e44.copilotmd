  1: { x: -100, y: 0 },    // what-i-do: top: 0vh, left: 100vw  
  2: { x: -100, y: -100 }, // project-1: top: 100vh, left: 100vw
  3: { x: -200, y: -100 }, // project-2: top: 100vh, left: 200vw
